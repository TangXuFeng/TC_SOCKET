or+i1+i2 1 0 r0 #计数器值
or+i1+i2 100 0 r1 #上限
and+i1+i2 0 0 r2 #累加结果
and+i1+i2 0 0 r3 #累加结果高位
label loop
add r0 r2 r2 #把计数器累加到结果
add+i1 1 r0 r0 #计数器+1
jb+i2 r2 100 next #r2<100跳过
add+i1 1 r3 r3 #超过100,r3累加1
sub+i2 r2 100 r2 #然后r2-100
label next
jbe r0 r1 loop #r0<=r1跳转

and r3 r3 io #输出结果
and r2 r2 io #输出结果

label end
je+i1+i2 0 0 end #死循环